        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/vert</item>
        <item name="colorPrimaryVariant">@color/jaune</item>
        <item name="colorOnPrimary">@color/blanc</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/gris</item>
